# A Master with Node's IP and Ansible root User
[masters]
#master ansible_host=master_ip ansible_user=root
master ansible_host=137.117.216.163 ansible_user=azureuser

# Two Workers and Ansible root User
[workers]
worker1 ansible_host=worker_1_ip ansible_user=root
worker2 ansible_host=worker_2_ip ansible_user=root

# Python 3 for management operations
[all:vars]
ansible_python_interpreter=/usr/bin/python3
